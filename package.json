{
  "name": "mydatepicker",
  "version": "1.7.5",
  "description": "Angular2 date picker",
  "keywords": [
    "Angular2",
    "datepicker",
    "date picker"
  ],
  "author": "kekeh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kekeh/mydatepicker.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js --progress",
    "build.dist": "gulp all",
    "build.webpack.sampleapp": "rimraf build-sampleapp && webpack --config config/webpack.build.sampleapp.js --progress --profile --bail",
    "test": "karma start config/karma.conf.js",
    "tslint": "gulp tslint",
    "ngc": "ngc -p tsconfig.publish.json",
    "bundle": "rollup -c rollup.config.js && uglifyjs npmdist/bundles/mydatepicker.umd.js -c --screw-ie8 -o npmdist/bundles/mydatepicker.umd.min.js"
  },
  "main": "bundles/mydatepicker.umd.js",
  "typings": "index.d.ts",
  "module": "index.js",
